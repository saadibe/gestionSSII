
<div class="panel panel-primary"><div class="panel-heading">Recherche</div>
<div class="panel-body"><div class='filters'>
    <div class='filter-container'>
	  <label>Id</label>
      <input autocomplete='off' class='filter' name='Id' placeholder='Id' data-col='Id'/>
    </div>
    <div class='filter-container'>
	  <label>Nom</label>
      <input autocomplete='off' class='filter' name='Nom' placeholder='drink' data-col='Nom'/>
    </div>
    <div class='filter-container'>
	  <label>Pr&eacute;nom</label>
      <input autocomplete='off' class='filter' name='Pr&eacute;nom' placeholder='Pr&eacute;nom' data-col='Pr&eacute;nom'/>
    </div>
    <div class='filter-container'>
	  <label>E-mail</label>
      <input autocomplete='off' class='filter' name='E-mail' placeholder='E-mail' data-col='E-mail'/>
    </div>
	<div class='filter-container'>
	  <label>Domaine de comp&eacute;tence</label>
      <input autocomplete='off' class='filter' name='Domaine de comp&eacute;tence' placeholder='Domaine de comp&eacute;tence' data-col='Domaine de comp&eacute;tence'/>
    </div>
	<div class='filter-container'>
	  <label>Note</label>
      <input autocomplete='off' class='filter' name='Note' placeholder='Note' data-col='Note'/>
    </div>
    <div class='clearfix'></div>
    <button type="submit" class="btn btn-inverse right-rounded">Chercher</button>
  </div></div></div>

<div class="gestion-container">
<div class="ajouter-candidat js-ajouter-candidat"></div>
<div class="panel panel-primary"><div class="panel-heading">Resultat</div>
<div class="panel-body">
<table class="table table-hover">
   <thead>
      <tr>
         <th width= "5%"><i class="" aria-hidden="true"></i>Check</th>
         <th width= "5%"><i class="" aria-hidden="true"></i>Id</th>
         <th width= "10%"><i class="" aria-hidden="true"></i>Nom</th>
         <th width= "10%"><i class="" aria-hidden="true"></i>Pr&eacute;nom</th>
         <th width= "15%"><i class="" aria-hidden="true"></i>E-mail</th>
         <th width= "20%"><i class="" aria-hidden="true"></i>Domaine de comp&eacute;tence</th>
         <th width= "5%"><i class="" aria-hidden="true"></i>Note</th>
         <th width= "30%"><i class="" aria-hidden="true"></i>Action</th>
      </tr>
   </thead>
   <tbody>
      {{#each candidats}}
       {{#if isactive}}         
      <tr class="clickable-row" data-candidat-id="{{idCandidat}}">
         <td class="check"><input type="checkbox" id="cbox1" value="checkbox1"></td>
         <td class="candidatid">{{idCandidat}}</td>
         <td class="firstName">{{firstName}}</td>
         <td class="">{{lastName}}</td>
         <td class="">{{email}}</td>
         <td class="">{{expertise}}</td>
         <td class=""></td>
         <td class="">
            <div class="afficher js-afficher-candidat" data-idCandidat="{{idCandidat}}"></div>
            <div class="update" data-idCandidat="{{idCandidat}}"></div>
            <div class="supprimer js-supprimer-candidat" data-idcandidat="{{idCandidat}}" ></div>
         </td>
      </tr>
      {{/if}}
      {{/each}}
   </tbody>
</table>
</div></div>
</div>