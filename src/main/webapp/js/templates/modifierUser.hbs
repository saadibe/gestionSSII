<div class="panel panel-primary"><div class="panel-heading">modification Utilisateur</div>
<div class="panel-body">
<div class="bootstrap-iso">
 <div class="container-fluid">
  <div class="row">
   <div class="col-md-6 col-sm-6 col-xs-12">
      <div class="form-group hiden" >
      <label class="control-label " for="userId">
       id
      </label>
      <input class="form-control" id="userId" name="userId" value={{user.userId}} type="text"   />
     </div>
     <div class="form-group firstName">
      <label class="control-label " for="firstName">
       nom
      </label>
      <input class="form-control" id="firstName" name="firstName" type="text" value={{user.firstName}}  />
      <span class="error-inline"></span>
     </div>
     <div class="form-group lastName">
      <label class="control-label " for="lastName" >
       prenom
      </label>
      <input class="form-control" id="lastName" name="lastName" type="text" value={{user.lastName}}  />
      <span class="error-inline"></span>
     </div>
     <div class="form-group ">
      <label class="control-label " for="sexe">
       sexe
      </label>
      <select class="select form-control" id="sexe" name="sexe"  >
      {{#each user.arraysexe}}
       <option >
       {{this}}
       </option>
       {{/each}}
      </select>
     </div>
      <div class="form-group ">
      <label class="control-label " for="profile">
       profile
      </label>
      <select class="select form-control" id="profile" name="profile" >
       {{#each user.arrayprofile}}
       <option >
       {{this}}
       </option>
       {{/each}}
      </select>
     </div>
     <div class="form-group email">
      <label class="control-label requiredField" for="email" >
       Email
       <span class="asteriskField">
        *
       </span>
      </label>
      <input class="form-control" id="email" name="email" type="text"  value={{user.email}} />
      <span class="error-inline"></span>
     </div>
     <div class="form-group">
      <div>
      <div class="form-group password">
      <label class="control-label " for="password">
       Password
      </label>
      <input class="form-control" id="password" name="password" type="text" value={{user.password}}  />
      <span class="error-inline"></span>
     </div>
      <button class="btn btn-primary js-annuller" name="Annuller" type="submit">
        Annuller
        </button>
        <button class="btn btn-primary js-modifier" name="Modifier" type="submit">
        Modifier
       </button>
      </div>
     </div>
   </div>
  </div>
 </div>
</div>
</div>
</div>